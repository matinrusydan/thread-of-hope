{
  "buildCommand": "pnpm prisma generate && pnpm next build",
  "installCommand": "pnpm install --frozen-lockfile",
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 10
    }
  },
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "true",
    "PRISMA_GENERATE_SKIP_DOWNLOAD": "true"
  },
  "regions": ["sin1"]
}